{
    "builds": [
        {
            "src": "createSubscription.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"(.*)",
            "dest":"createSubscription.js",
            "methods":["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
            "headers":{
                "Access-Control-Allow-Origin":"*"
            }
        }
    ]
}